[Script]
Dim ax, ay, DPIx, DPIy,ix
dim MyArray


_mian

Sub _mian
	
	

	_Dpi 
	_init 
	acc = InputBox("NUMBER?") 
	_RunWL 
	Delay 200
	_DpiSelect (1)
	Delay 200
	_MoveWindows (1)
	Delay 100

    
End Sub

Sub _Dpi
	DPIx =  1920
	DPIy = 	1080
End Sub

Sub _init
	acc=0
End Sub
Sub _RunWL
    RunApp "C:\WL\nwlom_english\wl.exe"
End Sub


Function _DpiSelect(ta)
    ax = 0
    ay = 0
    cl = 1

    While cl = 1
	XY=Plugin.Color.FindMutiColor(0,0,DPIx,DPIy,"FDFDFD","117|0|FDFDFD,144|0|B85F00,141|4|B85F00,147|8|B85F00,150|5|B85F00",1)
	MyArray = Split(XY, "|")
	X = CInt(MyArray(0)): Y = CInt(MyArray(1))
        If x > - 1  and y > - 1  Then 
        TracePrint "find!"
		TracePrint x
		TracePrint y
		MoveTo x, y
		LeftDown 1
		Delay 30
		LeftUp 1
		
		Delay 50
		
		MoveTo x, y + 40
		LeftDown 1
		Delay 30
		LeftUp 1
		
		Delay 50
		
		MoveTo x+140, y + 135
		LeftDown 1
		Delay 30
		LeftUp 1
		
		cl = 0
        Else 
            TracePrint "NOT FOUND _DpiSelect"
        End If
        If ta = 0 Then 
            cl = 0
        End If
    Wend
    

End Function	

Function _MoveWindows(ta)
    ax = 0
    ay = 0
    cl = 1

    While cl = 1
	XY=Plugin.Color.FindMutiColor(ax,ay,DPIx,DPIy,"000000","-21|0|84BDDC,-21|4|28A277,-21|8|30B894,-13|8|114CC8,-12|2|817395,-16|1|4C4C74,-16|7|206FDD",0.8)
	MyArray = Split(XY, "|")
	X = CInt(MyArray(0)): Y = CInt(MyArray(1))
        If x > - 1  and y > - 1  Then 
        TracePrint "find!"
		TracePrint x
		TracePrint y
		MoveTo x ,  y
		LeftDown 1
		Delay 100
		If acc < 5 Then 
		MoveTo (480*(acc-1)) + 27, 0	
		
		ElseIf acc > 4 and acc < 9 Then
		
		MoveTo (480 * (acc - 5)) + 27, 350
		
		ElseIf acc > 8 and acc < 13 Then
		
		MoveTo (480 * (acc - 9)) + 27, 700
		
		End If
		
		LeftUp 1
		
		cl = 0
        Else 
            TracePrint "NOT FOUND _MoveWindows"
        End If
        If ta = 0 Then 
            cl = 0
        End If
    Wend
    

End Function	
